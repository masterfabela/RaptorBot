{
  "name": "raptorbot",
  "version": "1.0.0",
  "description": "Bot que garda voces de tor",
  "main": "raptorBot.ts",
  "scripts": {
    "start": "tsc && pm2 start ./dist/raptorBot.js",
    "stop": "pm2 stop dist/raptorBot.js",
    "restart": "tsc && pm2 restart ./dist/raptorBot.js",
    "tsc": "tsc",
    "dev": "ts-node-dev --respawn --transpileOnly ./raptorBot.ts",
    "prod": "tsc && pm2 start ./dist/raptorBot.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/masterfabela/RaptorBot.git"
  },
  "keywords": [
    "telegram",
    "bot",
    "media"
  ],
  "author": "fabelaretorcida@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/masterfabela/RaptorBot/issues"
  },
  "homepage": "https://github.com/masterfabela/RaptorBot#readme",
  "dependencies": {
    "@types/node": "^13.7.1",
    "node-telegram-bot-api": "^0.40.0",
    "pm2": "^4.2.1",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.7.5"
  }
}
